  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.34.0",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
