{
  "name": "rest-client-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "format:fix": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "build": "react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint .",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "test:browser": "vitest --config=vitest.browser.config.ts --coverage.enabled=true"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
