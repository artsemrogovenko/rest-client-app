    "@types/react-dom": "^19.1.2",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "commitlint-plugin-function-rules": "^4.0.2",
    "eslint": "^9.34.0",
